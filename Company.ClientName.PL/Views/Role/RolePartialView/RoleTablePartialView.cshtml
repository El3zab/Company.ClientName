@model IEnumerable<RoleToReturnDto>



<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Details</th>
            <th>Update</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>

                <partial name="_ButtonPartialView" model="@item.Id" />
            </tr>
        }
    </tbody>
</table>
